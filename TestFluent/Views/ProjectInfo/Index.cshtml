
@{
    ViewBag.Title = "Index";
}

@using TestFluent.Models;
<h2>Index</h2>



<table class="table table-bordered table-condensed table-hover table-responsive table">
    <tr>
        <td>ProId</td>
        <td>ProjectName</td>
        <td>Pmo</td>
        <td>Operate</td>
    </tr>
    @foreach (ProjectInfo item in ViewBag.projectInfoList as List<ProjectInfo>)
    {
        <tr>
            <td>@item.ProId</td>
            <td>@item.ProjectName</td>
            <td>@item.Pmo</td>
            <td>
                <a href="Details/@item.ProId">详情</a>
                <a href="Delete/@item.ProId">删除</a>
            </td>
        </tr>
    }
</table>

















